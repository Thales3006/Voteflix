<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.thales.client.controller.VoteController">
   <center>
      <HBox prefHeight="600.0" prefWidth="1000.0">
         <children>
            <SplitPane orientation="VERTICAL" prefHeight="600.0" prefWidth="300.0" HBox.hgrow="ALWAYS">
               <items>
                  <Label text="Movies" />
               </items>
            </SplitPane>
            <SplitPane orientation="VERTICAL" prefHeight="600.0" prefWidth="300.0" HBox.hgrow="ALWAYS">
               <items>
                  <Label text="Review" />
               </items>
            </SplitPane>
         </children>
      </HBox>
   </center>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <top>
      <HBox alignment="TOP_RIGHT" prefHeight="50.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="logoutButton" mnemonicParsing="false" text="Logout" onAction="#HandleLogoutButton"/>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </top>
</BorderPane>
