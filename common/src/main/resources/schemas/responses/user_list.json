{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserListResponse",
  "type": "object",
  "properties": {
    "status": { "type": "string", "const": "200" },
    "usuarios": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "nome": { "$ref": "../components/base_string.json" }
        },
        "required": ["id", "nome"],
        "additionalProperties": false
      }
    }
  },
  "required": ["status", "usuarios"],
  "additionalProperties": false
}