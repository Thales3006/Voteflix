{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "EditarReview",
  "type": "object",
  "properties": {
    "operacao": { "type": "string", "const": "EDITAR_REVIEW" },
    "review": {
      "type": "object",
      "properties": {
        "id": { "type": "string" },
        "titulo": { "type": "string" },
        "descricao": { "type": "string" },
        "nota": { "type": "string", "pattern": "^[0-9]+$" }
      },
      "required": ["id", "titulo", "descricao", "nota"],
      "additionalProperties": false
    },
    "token": { "type": "string" }
  },
  "required": ["operacao", "review", "token"],
  "additionalProperties": false
}