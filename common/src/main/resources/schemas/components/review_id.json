{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Review with ID",
  "allOf": [
    { "$ref": "review.json" },
    {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "pattern": "^[0-9]+$"
        }
      },
      "required": ["id"]
    }
  ]
}